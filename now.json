{
  "version": 2,
  "builds": [
    { "src": "backend/index.ts", "use": "@now/node" },
    { "src": "package.json", "use": "@now/static-build" }
  ],
  "routes": [
    {
      "src": "/backend",
      "dest": "/backend/index.js"
    },
    {
      "src": "/",
      "dest": "/frontend/index.html"
    }
  ],
  "env": {
    "DATABASE_URL": "@database_url",
    "SOLVE_SECRET": "@solve_secret"
  }
}
